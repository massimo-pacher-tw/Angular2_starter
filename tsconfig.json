  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules",
    "__build__",
    "typings"
  ],
  "compileOnSave": false
}
